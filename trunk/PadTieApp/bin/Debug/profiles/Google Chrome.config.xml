<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" label="Google Chrome">
  <pad index="1">
    <a>
      <link type="PadTie.KeyAction">Return,</link>
      <tap type="none" />
      <dtap type="PadTie.KeyAction">T,Control</dtap>
      <hold type="none" />
      <overview>Return, DT: New Tab, H: History</overview>
    </a>
    <b>
      <link type="PadTie.KeyAction">Escape,</link>
      <tap type="none" />
      <dtap type="PadTie.KeyAction">W,Control</dtap>
      <hold type="none" />
      <overview>Escape, DT: Close Tab</overview>
    </b>
    <x>
      <link type="PadTie.KeyAction">Next,</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Page Down</overview>
    </x>
    <y>
      <link type="PadTie.KeyAction">PageUp,</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="PadTie.KeyAction">J,Control</hold>
      <overview>Page Up, H: Downloads</overview>
    </y>
    <back>
      <link type="PadTie.MouseButtonAction">Back</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Back</overview>
    </back>
    <start>
      <link type="PadTie.MouseButtonAction">Forward</link>
      <tap type="none" />
      <dtap type="PadTie.KeyAction">L,Control</dtap>
      <hold type="none" />
      <overview>T: Forward, DT: Location Bar</overview>
    </start>
    <system>
      <link type="none" />
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
    </system>
    <bl>
      <link type="PadTie.KeyAction">Next,Control</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Next Tab</overview>
    </bl>
    <br>
      <link type="PadTie.MouseButtonAction">Left</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Left Mouse</overview>
    </br>
    <tl>
      <link type="PadTie.KeyAction">PageUp,Control</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Previous Tab</overview>
    </tl>
    <tr>
      <link type="PadTie.MouseButtonAction">Right</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Right Mouse</overview>
    </tr>
    <la>
      <link type="PadTie.KeyAction">OemMinus,Control</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Zoom Out</overview>
    </la>
    <ra>
      <link type="PadTie.KeyAction">Oemplus,Control</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Zoom In</overview>
    </ra>
    <la-up type="PadTie.MouseWheelAction">
      <link type="PadTie.MouseWheelAction">120,True,True</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Scroll Up</overview>120,True,True</la-up>
    <la-down type="PadTie.MouseWheelAction">
      <link type="PadTie.MouseWheelAction">-120,True,True</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Scroll Down</overview>-120,True,True</la-down>
    <la-left type="PadTie.KeyAction">
      <link type="PadTie.KeyAction">Home,</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Top</overview>Home,</la-left>
    <la-right type="PadTie.KeyAction">
      <link type="PadTie.KeyAction">End,</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Bottom</overview>End,</la-right>
    <ra-up type="PadTie.MousePointerAction">
      <link type="PadTie.MousePointerAction">0,-25</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Mouse Up</overview>0,-25</ra-up>
    <ra-down type="PadTie.MousePointerAction">
      <link type="PadTie.MousePointerAction">0,25</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Mouse Down</overview>0,25</ra-down>
    <ra-left type="PadTie.MousePointerAction">
      <link type="PadTie.MousePointerAction">-25,0</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Mouse Left</overview>-25,0</ra-left>
    <ra-right type="PadTie.MousePointerAction">
      <link type="PadTie.MousePointerAction">25,0</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Mouse Right</overview>25,0</ra-right>
    <dig-up type="PadTie.KeyAction">
      <link type="PadTie.KeyAction">Up,</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Up</overview>Up,</dig-up>
    <dig-down type="PadTie.KeyAction">
      <link type="PadTie.KeyAction">Down,</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Down</overview>Down,</dig-down>
    <dig-left type="PadTie.KeyAction">
      <link type="PadTie.KeyAction">Left,</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Left</overview>Left,</dig-left>
    <dig-right type="PadTie.KeyAction">
      <link type="PadTie.KeyAction">Right,</link>
      <tap type="none" />
      <dtap type="none" />
      <hold type="none" />
      <overview>Right</overview>Right,</dig-right>
  </pad>
  <info>Layout Author: William Lahti\nThis layout is built-in to Pad Tie 0.1b2 and later.\n\nTurns your gamepad into a remote for your browser!</info>
</config>